<%if product%>
 <tr data-link="<%= product_path(product) %>">
 		<td ><span id=<%= product.id.to_s() +"#" +  product.name %> class='glyphicon glyphicon-plus'/></td>
        <td ><%= product.name %></td>
        <td ><%= product.description %></td>
        <td ><%= product.article_url %></td>
        <td ><%= link_to "Excel", product_path(product,format: "xls") %></td>
        <td><span id=<%= product.id.to_s() +"#" +  product.name %> class='glyphicon glyphicon-edit'/><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td> 
</tr>
<%end%>